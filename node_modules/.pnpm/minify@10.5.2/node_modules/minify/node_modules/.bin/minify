#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/59007026/dev/corymy/node_modules/.pnpm/minify@10.5.2/node_modules/minify/bin/node_modules:/Users/59007026/dev/corymy/node_modules/.pnpm/minify@10.5.2/node_modules/minify/node_modules:/Users/59007026/dev/corymy/node_modules/.pnpm/minify@10.5.2/node_modules:/Users/59007026/dev/corymy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/59007026/dev/corymy/node_modules/.pnpm/minify@10.5.2/node_modules/minify/bin/node_modules:/Users/59007026/dev/corymy/node_modules/.pnpm/minify@10.5.2/node_modules/minify/node_modules:/Users/59007026/dev/corymy/node_modules/.pnpm/minify@10.5.2/node_modules:/Users/59007026/dev/corymy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/minify.js" "$@"
else
  exec node  "$basedir/../../bin/minify.js" "$@"
fi
